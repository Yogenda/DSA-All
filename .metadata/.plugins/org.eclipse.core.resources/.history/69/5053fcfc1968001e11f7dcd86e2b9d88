package first;

public class FirstClass {
	public static void main(String args[]) {
		System.out.println("Hello world!"); // print line by line.
		
		int a = 1;
		System.out.print(a);
		int b = 4;
		System.out.println(b);
		System.out.println(b);
		byte z = 127;
		System.out.println("valu fo z"+z);
		float c = 3.5542368987f;
		double d = 4.9;
		// + can also be used a concatination and also for addition
		System.out.println(c +" "+d);
		
		System.out.println("Value of A is = "+a+" and value of B is = "+b);
		
	}
}
