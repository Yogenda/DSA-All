package first;

public class FirstClass {
	public static void main(String args[]) {
		System.out.println("Hello world!"); // print line by line.
		
		int a = 1;
		System.out.print(a);
		int b = 4;
		System.out.println(b);
		System.out.println(b);
		byte z = 127;
		System.out.println("valu fo z"+z);
		float c = 3.554236898756465456f;
		double d = 4.956123456789632587141;
		// + can also be used a concatination and also for addition
		System.out.println(c +" "+d);
		boolean t = true;
		boolean f = false;
		char ch = 105;
		System.out.println("Value of A is = "+a+" and value of B is = "+b);
		System.out.println(ch);
		
		a = 6;
		System.out.println(a);
		a = 79;
		System.out.println(a);
	}
}
